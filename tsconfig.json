{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strictNullChecks": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "sourceMap": true,
      "jsx": "react"
    },
    "include": ["src/**/*.tsx", "src/**/*.ts", "webpack.config.js"],
    "exclude": ["node_modules"]
  }