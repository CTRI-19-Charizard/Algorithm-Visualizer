{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "sourceMap": true,
      "jsx": "react",
      "strictNullChecks": true,
    },
    "include": ["src/**/*.tsx", "src/client/index.tsx"],
    "exclude": ["node_modules"]
  }